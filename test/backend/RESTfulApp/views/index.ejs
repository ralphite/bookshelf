<% include ./partials/header %>

<% blogs.forEach(function(blog) { %>
    <div>
        <h2><%= blog.title %></h2>
            <img src="<%= blog.image %>">
            <span><%= blog.created %></span>
            <p><%= blog.body.substring(0, 100)%>...</p>
            <a class="ui big violet basic button" href="/blogs/<%- blog._id %>">Show more</a>
    </div>

<% })%>
<% include ./partials/footer %>